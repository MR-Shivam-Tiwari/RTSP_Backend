{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/overlays/(.*)",
            "dest": "/overlays.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}
